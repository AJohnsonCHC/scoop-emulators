{
    "homepage": "http://www.fceux.com",
    "version": "2.2.3",
    "license": "GPL-2.0-only",
    "description": "FCEUX is a Nintendo Entertainment System (NES), Famicom, Famicom Disk System (FDS), and Dendy emulator.",
    "url": "https://downloads.sourceforge.net/project/fceultra/Binaries/2.2.3/fceux-2.2.3-win32.zip",
    "hash": "sha1:98e228bb654f4470356850041c54e44ca5156107",
    "shortcuts": [
        [
            "fceux.exe",
            "FCEUX"
        ]
    ],
    "pre_install": [
        "if (!(Test-Path \"$persist_dir\\fceux.cfg\")) {",
        "    New-Item \"$dir\\fceux.cfg\" -Type File | Out-Null",
        "}"
    ],
    "persist": [
        "cheats",
        "fcs",
        "movies",
        "sav",
        "snaps",
        "fceux.cfg"
    ],
    "checkver": {
        "url": "https://sourceforge.net/projects/fceultra/best_release.json",
        "jsonpath": "$.release.filename",
        "regex": "([\\d.]+)/fceux-[\\d.]+-win32\\.zip"
    },
    "autoupdate": {
        "url": "https://downloads.sourceforge.net/project/fceultra/Binaries/$version/fceux-$version-win32.zip"
    }
}
